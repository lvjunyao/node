@fontc:red;  // 字体颜色
@fonts:30px; // 字体大小
@backgroundC: yellow; // 背景颜色
@count:12;
@md-wi:970px;

.ttt{
  width:floor(@md-wi/@count*6);
  background:@backgroundC;
  @media(min-width:768px){
    background:gray;
    @media(min-resolution:52dpi){
      background-image:url(11.png)
    }
  }
  @base: #f04615;
  @media(min-width:1080px){
    background-color:spin(lighten(@base, 25%), 8);
  }
  .t1{
    .t3
  }
  &:after{
    content:'sadfsda';
    display:block;
    font-size:@fonts;
  }
  &:after{
    content:'1111';
    display:block;
    font-size:@fonts;
  }
  .t2{
    font-size:calc(@fonts*2);
  }
}
.t3{
    color:@fontc;
}
@min768:~"(min-width:768px)";
.d1{
  @base: #f04615;
  background-color:spin(lighten(@base, 25%), 8);
  @media @min768{
    font-size:@fonts;
  }
}
@base: #f04615;
.p1{
  background-color:@base
}
.p2{
  background-color:lighten(@base, 25%)
}
.p3{
  background-color:spin(lighten(@base, 25%), 8); 
}



#bundle() {
  .button {
    display: block;
    border: 1px solid black;
    background-color: grey;
    &:hover {
      background-color: white
    }
  }
  /* .tab { ... }
  .citation { ... }
  */
}

.p1{
 #bundle.button
}